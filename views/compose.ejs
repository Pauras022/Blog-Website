<%- include("partials/header.ejs") %>
<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
    <label for="composed">Title</label>
    <input class="form-control" type="text" name="postTitle" autocomplete="off" />
  </div>
  <div class="form-group">
    <label for="composed">Post</label>
    <textarea class="form-control" name="postBody" cols="30" rows="10"></textarea>
  </div>
  <button class="btn-primary" type="submit">Publish</button>
</form>
<%- include("partials/footer.ejs") %>
